---
layout: layouts/stickerbook.njk
permalink: /stickerbook/frame.html
---

<style>
    body { 
        background: none
    }
</style>

<div id="stickers" class="row">
  {% for sticker in post %}
    <div class="sticker" id="sticker-{{sticker.day}}">
      <img src="{{sticker.nativity_figure}}" draggable="false">
    </div>
  {% endfor %}
</div>

<div id="stable">
  <img src="/.netlify/images?url=img/stickers/stable_front.png" class="stable-bg" draggable="false">
</div>